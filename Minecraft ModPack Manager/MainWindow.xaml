<Window x:Class="Minecraft_ModPack_Manager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Minecraft_ModPack_Manager"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>

        <TextBlock Width="300" Height="20" Name="ErrorMessage" Margin="0,0,0,380"/>
        <TextBlock TextWrapping="Wrap" Width="500" Height="50" Name="Pathes" Margin="0,0,0,300" />
        <ComboBox SelectionChanged="InstanceSelector_SelectionChanged" Name="InstanceSelector" Visibility="Hidden" Width="200" Height="20"/>


    </Grid>


</Window>
